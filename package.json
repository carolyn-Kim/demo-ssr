{
  "name": "yd-ssr",
  "version": "1.0.0",
  "description": "vue-ssr搭建",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start ./dist/pm2.json",
    "build": "npm run client:dev && npm run client:server && npm run gulp:dev",
    "prod": "npm run gulp:prod && npm run client:prod && npm run client:server",
    "gulp:dev": "cross-env NODE_ENV=development gulp",
    "gulp:prod": "cross-env NODE_ENV=production gulp",
    "gulp:lint": "cross-env NODE_ENV=lint gulp",
    "client:dev": "cross-env NODE_ENV=development webpack --config ./config/webpack.dev.js --progress --colors",
    "client:server": "cross-env NODE_ENV=development webpack --config ./config/webpack.server.js --progress --hide-modules",
    "client:prod": "cross-env NODE_ENV=development webpack --config ./config/webpack.prod.js --progress --hide-modules",
    "start:dev": "cross-env NODE_ENV=development supervisor ./dist/app.js",
    "start:prod": "cross-env NODE_ENV=production supervisor ./dist/app.js",
    "e2e": "node ./tests/e2e/rizeRunner.js",
    "service": "mocha ./tests/mochaRunner.js"
  },
  "keywords": [],
  "author": "kim",
  "license": "ISC",
